{
  "compilerOptions": {
    "alwaysStrict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es6",
    "outDir": "out",
    "lib": [
      "es6"
    ],
    "sourceMap": true,
    "rootDir": "."
  },
  "exclude": [
    "integrations",
    "__mocks__"
  ]
}